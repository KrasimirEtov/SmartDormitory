@model CreateUpdateSensorViewModel

<div class="row">
    <div class="col-md-2">
        <h4><span class="form-row badge badge-pill badge-light text-secondary"> Creating sensor </span></h4>
    </div>

    <div class="col-md-2">
        <h4><span class="h5 badge badge-pill badge-warning"> Step 2: </span></h4>
    </div>
    <div class="col-md-4">
        <h4><span class="h5 badge badge-pill badge-info"> Add sensor details </span></h4>
    </div>
    <div class="col-md-4">
        <a asp-area="Administration" asp-controller="Sensor" asp-action="RegisterIndex" asp-route-userId="@Model.UserId">
            <h4><span class="h5 badge badge-pill badge-primary">Go Back</span></h4>
        </a>
    </div>
</div>

<hr />

<partial name="_InputCreateSensorPartial" model="Model" />